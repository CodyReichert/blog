!!! 5
%html
  %head
    <meta charset="utf-8" />
    <meta http-equiv='X-UA-Compatible' content='IE=edge;chrome=1' />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    %title
      = "Cody Reichert's Blog"

    %link{:type => "text/css", :rel => "stylesheet", :href => "/stylesheets/all.css"}

  %body

    #header
      = partial(:sidebar)

    .row-fluid
      .col-md-9.col-md-offset-3.content
        #contentupper
          = partial(:contentupper)

    .row-fluid
      .col-md-9.col-md-offset-3.content
        #main{:role => "main"}
          = yield

    .col-md-9.col-md-offset-3.content
      %p{ :style => 'text-align:right;' } Copyright 2014 - Cody Reichert

    %script(src="/javascripts/all.js")
